{
  "name": "template-next",
  "description": "Full template for Next js 10 project which works with both js and ts",
  "version": "0.1.0",
  "private": true,
  "keywords": [
    "jest",
    "eslint",
    "stylelint",
    "prettier",
    "redux",
    "next-redux-wrapper",
    "react-intl",
    "react-query",
    "lodash",
    "luxon",
    "sass",
    "husky",
    "lint-staged"
  ],
  "author": {
    "name": "Houssem Ghorbel",
    "email": "houssemgho@gmail.com"
  },
  "scripts": {
    "dev": "next dev",
    "lint:staged": "lint-staged",
    "build": " pnpm run lint:staged && next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint:style": "stylelint '**/*.{css,scss,sass}' --fix",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "pre-push": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,scss,sass}": [
      "prettier --write",
      "stylelint '**/*.{css,scss,sass}' --fix"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.18.9",
    "babel-plugin-transform-define": "^2.0.1",
    "dotenv": "^10.0.0",
    "error": "10.4.0",
    "husky-init": "^7.0.0",
    "lodash": "^4.17.21",
    "luxon": "^1.27.0",
    "next": "10.2.2",
    "next-redux-wrapper": "^6.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "^5.22.0",
    "react-query": "^3.16.0",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "sass": "^1.33.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.0",
    "@types/react": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-core": "^6.26.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
